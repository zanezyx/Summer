<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/fragment_around_background">
    <item >
        <selector >
            <item android:state_enabled="true" android:drawable="@drawable/button_state_start"/>
            <item android:state_focused="true" android:drawable="@drawable/button_state_start"/>
            <item android:state_pressed="true" android:drawable="@drawable/button_state_start"/>
            <item android:state_enabled="false" android:drawable="@drawable/button_state_disable"/>
            <item android:drawable="@drawable/button_state_disable"/>
        </selector>
    </item>

</ripple>
